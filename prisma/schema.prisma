generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cars {
  id          String  @id @default(uuid())
  name        String  @db.VarChar(255)
  description String? @db.VarChar(255)
  brand       String  @db.VarChar(255)
  year        Int     @db.SmallInt()
  km          Decimal @db.Decimal()
}
